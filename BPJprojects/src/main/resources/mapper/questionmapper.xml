<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao_questiondao">
   
  <insert id="insertquestion">
       insert into question values(#{id},#{title},#{description},#{gmt_create},#{gmt_modify},#{createid},#{comment_count},#{view_count},#{like_count},#{tag})
  </insert>
  <select id="equestion" resultType="com.example.demo.entity_question">
      select * from question
  </select>
 
</mapper>